<Application x:Class="Graphium.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Graphium"
             xmlns:vm="clr-namespace:Graphium.ViewModels"
             xmlns:v ="clr-namespace:Graphium.Views"
             xmlns:c ="clr-namespace:Graphium.Controls">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
            </ResourceDictionary.MergedDictionaries>
            <DataTemplate DataType="{x:Type vm:ChannelsConfigViewModel}">
                <c:ChannelConfigControl/>
            </DataTemplate>
        </ResourceDictionary>
    </Application.Resources>
</Application>
